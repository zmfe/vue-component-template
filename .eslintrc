{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-tabs": 0,
    "no-new": 0,
    "no-param-reassign": "off",
    "quote-props": "off",
    "valid-jsdoc": [
      "error",
      {
        "requireReturn": false
      }
    ]
  }
}
